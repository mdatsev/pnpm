#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/cpy-cli/2.0.0/node_modules/cpy-cli/cli.js/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/cpy-cli/2.0.0/node_modules/cpy-cli/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/cpy-cli/2.0.0/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/cpy-cli/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules:/home/marti/dev/testing_deploy/node_modules:/home/marti/dev/node_modules:/home/marti/node_modules:/home/node_modules:/node_modules:/home/marti/dev/testing_deploy/pnpm/packages/fetch/node_modules/cpy-cli/cli.js/node_modules:/home/marti/dev/testing_deploy/pnpm/packages/fetch/node_modules/cpy-cli/node_modules:/home/marti/dev/testing_deploy/pnpm/packages/fetch/node_modules:/home/marti/dev/testing_deploy/pnpm/packages/node_modules" "$basedir/node"  "$basedir/../cpy-cli/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/cpy-cli/2.0.0/node_modules/cpy-cli/cli.js/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/cpy-cli/2.0.0/node_modules/cpy-cli/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/cpy-cli/2.0.0/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/cpy-cli/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules:/home/marti/dev/testing_deploy/node_modules:/home/marti/dev/node_modules:/home/marti/node_modules:/home/node_modules:/node_modules:/home/marti/dev/testing_deploy/pnpm/packages/fetch/node_modules/cpy-cli/cli.js/node_modules:/home/marti/dev/testing_deploy/pnpm/packages/fetch/node_modules/cpy-cli/node_modules:/home/marti/dev/testing_deploy/pnpm/packages/fetch/node_modules:/home/marti/dev/testing_deploy/pnpm/packages/node_modules" node  "$basedir/../cpy-cli/cli.js" "$@"
  ret=$?
fi
exit $ret
