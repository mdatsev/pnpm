#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/envinfo/7.4.0/node_modules/envinfo/dist/cli.js/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/envinfo/7.4.0/node_modules/envinfo/dist/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/envinfo/7.4.0/node_modules/envinfo/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/envinfo/7.4.0/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/envinfo/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules:/home/marti/dev/testing_deploy/node_modules:/home/marti/dev/node_modules:/home/marti/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../envinfo/7.4.0/node_modules/envinfo/dist/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/envinfo/7.4.0/node_modules/envinfo/dist/cli.js/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/envinfo/7.4.0/node_modules/envinfo/dist/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/envinfo/7.4.0/node_modules/envinfo/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/envinfo/7.4.0/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/envinfo/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules:/home/marti/dev/testing_deploy/node_modules:/home/marti/dev/node_modules:/home/marti/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../envinfo/7.4.0/node_modules/envinfo/dist/cli.js" "$@"
  ret=$?
fi
exit $ret
