#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/verdaccio/4.3.5/node_modules/verdaccio/bin/verdaccio/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/verdaccio/4.3.5/node_modules/verdaccio/bin/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/verdaccio/4.3.5/node_modules/verdaccio/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/verdaccio/4.3.5/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/verdaccio/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules:/home/marti/dev/testing_deploy/node_modules:/home/marti/dev/node_modules:/home/marti/node_modules:/home/node_modules:/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/verdaccio/bin/verdaccio/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/verdaccio/bin/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/verdaccio/node_modules" "$basedir/node"  "$basedir/../verdaccio/bin/verdaccio" "$@"
  ret=$?
else 
  NODE_PATH="/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/verdaccio/4.3.5/node_modules/verdaccio/bin/verdaccio/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/verdaccio/4.3.5/node_modules/verdaccio/bin/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/verdaccio/4.3.5/node_modules/verdaccio/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/verdaccio/4.3.5/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/verdaccio/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules:/home/marti/dev/testing_deploy/node_modules:/home/marti/dev/node_modules:/home/marti/node_modules:/home/node_modules:/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/verdaccio/bin/verdaccio/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/verdaccio/bin/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/verdaccio/node_modules" node  "$basedir/../verdaccio/bin/verdaccio" "$@"
  ret=$?
fi
exit $ret
