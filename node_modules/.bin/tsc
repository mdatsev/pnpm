#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.2/node_modules/typescript/bin/tsc/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.2/node_modules/typescript/bin/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.2/node_modules/typescript/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.2/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/typescript/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules:/home/marti/dev/testing_deploy/node_modules:/home/marti/dev/node_modules:/home/marti/node_modules:/home/node_modules:/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/typescript/bin/tsc/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/typescript/bin/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/typescript/node_modules" "$basedir/node"  "$basedir/../typescript/bin/tsc" "$@"
  ret=$?
else 
  NODE_PATH="/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.2/node_modules/typescript/bin/tsc/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.2/node_modules/typescript/bin/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.2/node_modules/typescript/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.2/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/typescript/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules:/home/marti/dev/testing_deploy/node_modules:/home/marti/dev/node_modules:/home/marti/node_modules:/home/node_modules:/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/typescript/bin/tsc/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/typescript/bin/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/typescript/node_modules" node  "$basedir/../typescript/bin/tsc" "$@"
  ret=$?
fi
exit $ret
