#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/ts-node/8.5.2_typescript@3.7.2/node_modules/ts-node/dist/script.js/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/ts-node/8.5.2_typescript@3.7.2/node_modules/ts-node/dist/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/ts-node/8.5.2_typescript@3.7.2/node_modules/ts-node/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/ts-node/8.5.2_typescript@3.7.2/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/ts-node/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules:/home/marti/dev/testing_deploy/node_modules:/home/marti/dev/node_modules:/home/marti/node_modules:/home/node_modules:/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/ts-node/dist/script.js/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/ts-node/dist/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/ts-node/node_modules" "$basedir/node"  "$basedir/../ts-node/dist/script.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/ts-node/8.5.2_typescript@3.7.2/node_modules/ts-node/dist/script.js/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/ts-node/8.5.2_typescript@3.7.2/node_modules/ts-node/dist/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/ts-node/8.5.2_typescript@3.7.2/node_modules/ts-node/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/ts-node/8.5.2_typescript@3.7.2/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/ts-node/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/.pnpm/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules:/home/marti/dev/testing_deploy/node_modules:/home/marti/dev/node_modules:/home/marti/node_modules:/home/node_modules:/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/ts-node/dist/script.js/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/ts-node/dist/node_modules:/home/marti/dev/testing_deploy/pnpm/node_modules/ts-node/node_modules" node  "$basedir/../ts-node/dist/script.js" "$@"
  ret=$?
fi
exit $ret
